@article{Rajpurkar2022,
	title        = {AI in health and medicine},
	author       = {Pranav Rajpurkar and Emma Chen and Oishi Banerjee and Eric J. Topol},
	year         = {2022},
	month        = {1},
	journal      = {Nature Medicine},
	publisher    = {Nature Research},
	volume       = {28},
