isbn = {978-1-4503-2744-2},
issn = {09255273},
keywords = {network dynamics,networks of diffusion,twitter},
pages = {913--924},
title = {{The bursty dynamics of the Twitter information network}},
url = {http://dl.acm.org/citation.cfm?id=2566486.2568043},
year = {2014}
}
