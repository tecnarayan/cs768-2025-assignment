	pages = {e0170298},
	file = {Full Text PDF:/home/thom/Zotero/storage/6T3NCSXX/Shan et al. - 2017 - miR 1296-5p Inhibits the Migration and Invasion of.pdf:application/pdf;Snapshot:/home/thom/Zotero/storage/T3CZDL9I/article.html:text/html}
