	author = {He, Qing and Gao, Yuan and Jie, Zhuye and Yu, Xinlei and Laursen, Janne Marie and Xiao, Liang and Li, Ying and Li, Lingling and Zhang, Faming and Feng, Qiang and Li, Xiaoping and Yu, Jinghong and Liu, Chuan and Lan, Ping and Yan, Ting and Liu, Xin and Xu, Xun and Yang, Huanming and Wang, Jian and Madsen, Lise and Brix, Susanne and Wang, Jianping and Kristiansen, Karsten and Jia, Huijue},
	year = {2017},
