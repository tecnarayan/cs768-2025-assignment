	keywords = {Animals, Caenorhabditis elegans, Cells, Cultured, Fibroblasts, Foreskin, Gene Dosage, Gene Expression, Humans, In Situ Hybridization, Fluorescence, Male, Molecular Sequence Data, S Phase, Sequence Analysis, RNA, Single-Cell Analysis, Transcription, Genetic},
	pages = {339--352}
}

