@techreport{azevedo_strategy-proofness_2015,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {Strategy-{Proofness} in the {Large}},
	url = {http://papers.ssrn.com/abstract=2237425},
	abstract = {We propose a criterion of approximate strategy-proofness, strategy-proofness in the large (SP-L), and argue that it is a useful second-best to exact strategy-proofness (SP) for market design. Our argument has three parts. First, in large markets, SP-L approximates several formal advantages of SP over Bayes-Nash incentive compatibility, namely, compliance with the Wilson doctrine, strategic simplicity, and fair treatment of unsophisticated participants. Second, our classification of existing non-SP mechanisms into those that are SP-L and those that are not organizes the evidence, from prior theoretical and empirical analyses of specific non-SP mechanisms, on when nonstrategy- proofness matters in real-world large markets. Last, in a class of environments that includes market design problems such as assignment, auctions and matching, under a set of assumptions including private values and anonymity, we show that SP-L is approximately costless to impose relative to Bayes-Nash incentive compatibility. We interpret these results as justifying SP-L as a second-best for market design: SP-L approximates the benefits of SP, but at approximately no cost relative to Bayes-Nash.},
	number = {ID 2237425},
	urldate = {2016-01-23},
	institution = {Social Science Research Network},
	author = {Azevedo, Eduardo M. and Budish, Eric B.},
	year = {2015},
