	file = {Full Text PDF:/home/thom/Zotero/storage/ZVJP3ZBD/Palarea-Albaladejo et al. - 2012 - Dealing with Distances and Transformations for Fuz.pdf:application/pdf;Snapshot:/home/thom/Zotero/storage/DITVHGBX/s00357-012-9105-4.html:text/html}
}

