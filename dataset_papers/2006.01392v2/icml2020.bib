@article{lovell_proportionality:_2015,
	title = {Proportionality: {A} {Valid} {Alternative} to {Correlation} for {Relative} {Data}},
	volume = {11},
	issn = {1553-734X},
	shorttitle = {Proportionality},
	doi = {10.1371/journal.pcbi.1004075},
	abstract = {In the life sciences, many measurement methods yield only the relative abundances of different components in a sample. With such relative{\textemdash}or compositional{\textemdash}data, differential expression needs careful interpretation, and correlation{\textemdash}a statistical workhorse for analyzing pairwise relationships{\textemdash}is an inappropriate measure of association. Using yeast gene expression data we show how correlation can be misleading and present proportionality as a valid alternative for relative data. We show how the strength of proportionality between two variables can be meaningfully and interpretably described by a new statistic $\phi$ which can be used instead of correlation as the basis of familiar analyses and visualisation methods, including co-expression networks and clustered heatmaps. While the main aim of this study is to present proportionality as a means to analyse relative data, it also raises intriguing questions about the molecular mechanisms underlying the proportional regulation of a range of yeast genes., Relative abundance data is common in the life sciences, but appreciation that it needs special analysis and interpretation is scarce. Correlation is popular as a statistical measure of pairwise association but should not be used on data that carry only relative information. Using timecourse yeast gene expression data, we show how correlation of relative abundances can lead to conclusions opposite to those drawn from absolute abundances, and that its value changes when different components are included in the analysis. Once all absolute information has been removed, only a subset of those associations will reliably endure in the remaining relative data, specifically, associations where pairs of values behave proportionally across observations. We propose a new statistic $\phi$ to describe the strength of proportionality between two variables and demonstrate how it can be straightforwardly used instead of correlation as the basis of familiar analyses and visualization methods.},
	number = {3},
	urldate = {2015-12-22},
	journal = {PLoS Computational Biology},
	author = {Lovell, David and Pawlowsky-Glahn, Vera and Egozcue, Juan Jos{\'e} and Marguerat, Samuel and B{\"a}hler, J{\"u}rg},
	month = mar,
	year = {2015},
	pmid = {25775355},
	pmcid = {PMC4361748},
	keywords = {Computational Biology, Models, Genetic, RNA, Messenger, Models, Statistical, Research Design, RNA, Fungal, Gene Expression Regulation, Fungal, Yeasts},
	file = {PubMed Central Full Text PDF:/home/thom/Zotero/storage/ETS97N2B/Lovell et al. - 2015 - Proportionality A Valid Alternative to Correlatio.pdf:application/pdf}
}

