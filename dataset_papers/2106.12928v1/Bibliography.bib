@inproceedings{SZTK08,
	Abstract = {Scope of this paper is to analyse four subcarrier allocation algorithms for multicellular {OFDMA} networks, namely coordinated, sequential, random and an innovative technique of cell splitting, that uses both random and coordinated subcarrier allocation. Common characteristic of these subcarrier allocation algorithms is that they do not require knowledge of channel state information ({CSI}) at the transmitter, a fact that leads to more efficient use of available bandwidth, lower algorithmic complexity and faster decision making. The algorithms are studied for an {OFDMA} multicellular network. Simulation results show that a multicellular {OFDMA} network is able to provide real broadband wireless access, with offered bit rate to reach 20 Mbps per cell, even without {CSI} knowledge.},
	Author = {Ioannis Stiakogiannakis and Dimitra Zarbouti and George Tsoulos and Dimitra Kaklamani},
