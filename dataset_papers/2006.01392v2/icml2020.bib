	author = {Chen, Yifei and Li, Yi and Narayan, Rajiv and Subramanian, Aravind and Xie, Xiaohui},
	year = {2016},
	pmid = {26873929},
	pmcid = {PMC4908320},
	keywords = {Gene Expression, Gene Expression Profiling, RNA, Linear Models, Machine Learning},
	pages = {1832--1839},
	file = {Full Text:/home/thom/Zotero/storage/BHYV2PJC/Chen et al. - 2016 - Gene expression inference with deep learning.pdf:application/pdf}
