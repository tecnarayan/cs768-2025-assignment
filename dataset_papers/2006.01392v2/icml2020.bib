	keywords = {Architectural distortion, Asymmetry, Breast cancer, Breast masses, Breast tumors, Calcifications, Computer-aided diagnosis, Enhancement, Mammography, Segmentation},
	pages = {312--348},
	file = {ScienceDirect Full Text PDF:/home/thom/Zotero/storage/PLQS8IX4/Rangayyan et al. - 2007 - A review of computer-aided diagnosis of breast can.pdf:application/pdf;ScienceDirect Snapshot:/home/thom/Zotero/storage/NJUXZX4F/S001600320600127X.html:text/html}
