	author = {Tylee, Daniel S. and Hess, Jonathan L. and Quinn, Thomas P. and Barve, Rahul and Huang, Hailiang and Zhang-James, Yanli and Chang, Jeffrey and Stamova, Boryana S. and Sharp, Frank R. and Hertz-Picciotto, Irva and Faraone, Stephen V. and Kong, Sek Won and Glatt, Stephen J.},
	month = apr,
