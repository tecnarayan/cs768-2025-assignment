@article{tsagris_data-based_2011,
	title = {A data-based power transformation for compositional data},
	abstract = {Compositional data analysis is carried out either by neglecting the compositional constraint and applying standard multivariate data analysis, or by transforming the data using the logs of the ratios of the components. In this work we examine a more general transformation which includes both approaches as special cases. It is a power transformation and involves a single parameter, \{{\textbackslash}alpha\}. The transformation has two equivalent versions. The first is the stay-in-the-simplex version, which is the power transformation as defined by Aitchison in 1986. The second version, which is a linear transformation of the power transformation, is a Box-Cox type transformation. We discuss a parametric way of estimating the value of \{{\textbackslash}alpha\}, which is maximization of its profile likelihood (assuming multivariate normality of the transformed data) and the equivalence between the two versions is exhibited. Other ways include maximization of the correct classification probability in discriminant analysis and maximization of the pseudo R-squared (as defined by Aitchison in 1986) in linear regression. We examine the relationship between the \{{\textbackslash}alpha\}-transformation, the raw data approach and the isometric log-ratio transformation. Furthermore, we also define a suitable family of metrics corresponding to the family of \{{\textbackslash}alpha\}-transformation and consider the corresponding family of Frechet means.},
	urldate = {2017-04-28},
	journal = {arXiv:1106.1451 [stat]},
	author = {Tsagris, Michail T. and Preston, Simon and Wood, Andrew T. A.},
	month = jun,
	year = {2011},
	note = {arXiv: 1106.1451},
	keywords = {Statistics - Methodology},
	file = {arXiv\:1106.1451 PDF:/home/thom/Zotero/storage/AVFN55JF/Tsagris et al. - 2011 - A data-based power transformation for compositiona.pdf:application/pdf;arXiv.org Snapshot:/home/thom/Zotero/storage/F3ZT3D4J/1106.html:text/html}
}

@article{gloor_displaying_2016,
