issn = {14712105},
journal = {BMC Bioinformatics},
keywords = {Biological network,Centrality,Economic payoff,Microbial social network},
pmid = {28617231},
title = {{ATria: A novel centrality algorithm applied to biological networks}},
year = {2017}
}
