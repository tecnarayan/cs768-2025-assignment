@article{duvallet_meta-analysis_2017,
	title = {Meta-analysis of gut microbiome studies identifies disease-specific and shared responses},
	volume = {8},
	copyright = {2017 The Author(s)},
	issn = {2041-1723},
	doi = {10.1038/s41467-017-01973-8},
	abstract = {Reported associations between the human microbiome and disease are often inconsistent. Here, Duvallet et al. perform a meta-analysis of 28 gut microbiome studies spanning ten diseases, and find associations that are likely not disease-specific but potentially part of a shared response to disease.},
