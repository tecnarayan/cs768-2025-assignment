	pages = {722--727},
	file = {IEEE Xplore Abstract Record:/home/thom/Zotero/storage/V7DHSFB3/5360301.html:text/html;IEEE Xplore Full Text PDF:/home/thom/Zotero/storage/WP5RUWUZ/Budhaditya et al. - 2009 - Effective Anomaly Detection in Sensor Networks Dat.pdf:application/pdf}
