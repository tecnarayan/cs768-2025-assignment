@article{gelfand_recursive_1991,
	title = {Recursive {Stochastic} {Algorithms} for {Global} {Optimization} in \${\textbackslash}mathbb\{{R}\}{\textasciicircum}d \$},
	volume = {29},
	issn = {0363-0129},
	url = {http://epubs.siam.org/doi/abs/10.1137/0329055},
	doi = {10.1137/0329055},
	abstract = {An algorithm of the form \$X\_\{k + 1\}  = X\_k  - a\_k ({\textbackslash}nabla U(X\_k ) + {\textbackslash}xi \_k ) + b\_k W\_k \$, where \$U( {\textbackslash}cdot )\$ is a smooth function on \${\textbackslash}mathbb\{R\}{\textasciicircum}d \$, \${\textbackslash}\{ {\textbackslash}xi \_k {\textbackslash}\} \$ is a sequence of \${\textbackslash}mathbb\{R\}{\textasciicircum}d \$-valued random variables, \${\textbackslash}\{ W\_k {\textbackslash}\} \$ is a sequence of independent standard d-dimensional Gaussian random variables, \$a\_k  = \{A / k\}\$ and \$b\_k  = \{\{{\textbackslash}sqrt B \} / \{{\textbackslash}sqrt \{k{\textbackslash}log {\textbackslash}log k\} \}\}\$ for k large, is considered. An algorithm of this type arises by adding slowly decreasing white Gaussian noise to a stochastic gradient algorithm. It is shown, under suitable conditions on \$U( {\textbackslash}cdot )\$, \${\textbackslash}\{ {\textbackslash}xi \_k {\textbackslash}\} \$, A, and B, that \$X\_k \$ converges in probability to the set of global minima of \$U( {\textbackslash}cdot )\$. No prior information is assumed as to what bounded region contains a global minimum. The analysis is based on the asymptotic behavior of the related diffusion process \$dY(t) =  - {\textbackslash}nabla U(Y(t))dt + c(t)dW(t)\$, where \$W( {\textbackslash}cdot )\$ is a standard d-dimensional Wiener process and \$c(t) = \{\{{\textbackslash}sqrt C \} / \{{\textbackslash}sqrt \{{\textbackslash}log t\} \}\}\$ for t large.},
	number = {5},
	urldate = {2017-01-20},
	journal = {SIAM J. Control Optim.},
	author = {Gelfand, S. and Mitter, S.},
	month = sep,
	year = {1991},
	pages = {999--1018},
	file = {Full Text PDF:/Users/ngolowich/Library/Application Support/Zotero/Profiles/7c93qi5d.default/zotero/storage/DECZHI93/Gelfand and Mitter - 1991 - Recursive Stochastic Algorithms for Global Optimiz.pdf:application/pdf;Gelfand91.pdf:/Users/ngolowich/Library/Application Support/Zotero/Profiles/7c93qi5d.default/zotero/storage/JJK2TRIBGe/lfand91.pdf:application/pdf;Snapshot:/Users/ngolowich/Library/Application Support/Zotero/Profiles/7c93qi5d.default/zotero/storage/5QG6RBEN/0329055.html:text/html}
}


@article{mandt_variational_2016,
	title = {A {Variational} {Analysis} of {Stochastic} {Gradient} {Algorithms}},
	url = {http://arxiv.org/abs/1602.02666},
	abstract = {Stochastic Gradient Descent (SGD) is an important algorithm in machine learning. With constant learning rates, it is a stochastic process that, after an initial phase of convergence, generates samples from a stationary distribution. We show that SGD with constant rates can be effectively used as an approximate posterior inference algorithm for probabilistic modeling. Specifically, we show how to adjust the tuning parameters of SGD such as to match the resulting stationary distribution to the posterior. This analysis rests on interpreting SGD as a continuous-time stochastic process and then minimizing the Kullback-Leibler divergence between its stationary distribution and the target posterior. (This is in the spirit of variational inference.) In more detail, we model SGD as a multivariate Ornstein-Uhlenbeck process and then use properties of this process to derive the optimal parameters. This theoretical framework also connects SGD to modern scalable inference algorithms; we analyze the recently proposed stochastic gradient Fisher scoring under this perspective. We demonstrate that SGD with properly chosen constant rates gives a new way to optimize hyperparameters in probabilistic models.},
	urldate = {2017-01-18},
	journal = {arXiv:1602.02666 [cs, stat]},
	author = {Mandt, Stephan and Hoffman, Matthew D. and Blei, David M.},
	month = feb,
	year = {2016},
	note = {arXiv: 1602.02666},
	keywords = {Computer Science - Learning, Statistics - Machine Learning},
	annote = {Comment: 8 pages, 3 figures},
	file = {1602.02666.pdf:files/503/1602.02666.pdf:application/pdf;arXiv\:1602.02666 PDF:files/513/Mandt et al. - 2016 - A Variational Analysis of Stochastic Gradient Algo.pdf:application/pdf;arXiv.org Snapshot:files/514/1602.html:text/html}
}

