year = {2013}
}
@inproceedings{Horel2016,
abstract = {We study the problem of maximizing a function that is approximately submodular under a cardinality constraint. Approximate submodularity implicitly appears in a wide range of applications as in many cases errors in evaluation of a submodular function break submodularity. Say that {\$}F{\$} is {\$}\backslasheps{\$}-approximately submodular if there exists a submodular function {\$}f{\$} such that {\$}(1-\backslasheps)f(S) \backslashleq F(S)\backslashleq (1+\backslasheps)f(S){\$} for all subsets {\$}S{\$}. We are interested in characterizing the query-complexity of maximizing {\$}F{\$} subject to a cardinality constraint {\$}k{\$} as a function of the error level {\$}\backslasheps {\textgreater} 0{\$}. We provide both lower and upper bounds: for {\$}\backslasheps {\textgreater} n{\^{}}{\{}-1/2{\}}{\$} we show an exponential query-complexity lower bound. In contrast, when {\$}\backslasheps {\textless} {\{}1{\}}/{\{}k{\}}{\$} or under a stronger bounded curvature assumption, we give constant approximation algorithms.},
