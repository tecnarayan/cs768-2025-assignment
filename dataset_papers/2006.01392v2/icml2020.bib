	pmcid = {PMC2267703},
	keywords = {Computer Simulation, Animals, Humans, Male, Mice, Gene Expression Profiling, Female, Databases, Genetic, Models, Genetic, Brain, Pan troglodytes, Species Specificity, Systems Biology, Tissue Distribution},
	pages = {54}
}

