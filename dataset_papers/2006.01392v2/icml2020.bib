	journal = {Clinical Medicine Insights. Oncology},
	author = {Inic, Zorka and Zegarac, Milan and Inic, Momcilo and Markovic, Ivan and Kozomara, Zoran and Djurisic, Igor and Inic, Ivana and Pupic, Gordana and Jancic, Snezana},
