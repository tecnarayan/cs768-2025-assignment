	pmid = {24755471},
	pages = {3238--3247},
	file = {Full Text PDF:/home/thom/Zotero/storage/XYQS6GY6/Mouradov et al. - 2014 - Colorectal Cancer Cell Lines Are Representative Mo.pdf:application/pdf;Snapshot:/home/thom/Zotero/storage/AXQ65IE8/3238.html:text/html}
