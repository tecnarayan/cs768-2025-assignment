@INPROCEEDINGS{Della_etal_2011development,
  author = {Della Toffola, Luca and Patel, Shyamal and Chen, Bor-rong and Ozsecen,
	Yalgin M and Puiatti, Alessandro and Bonato, Paolo},
  title = {Development of a platform to combine sensor networks and home robots
