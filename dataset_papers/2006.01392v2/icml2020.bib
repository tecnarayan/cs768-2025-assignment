	keywords = {Humans, Male, Models, Statistical, Statistics, Nonparametric, Analysis of Variance, Dose Fractionation, Prostatic Neoplasms, Survival Analysis, United States},
	pages = {164--172}
}

@article{graffelman_compositional_2017,
