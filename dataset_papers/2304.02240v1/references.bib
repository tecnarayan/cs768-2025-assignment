@misc{noauthor_hajoss_2021,
	title = {Hajós's theorem},
	copyright = {Creative Commons Attribution-ShareAlike License},
	url = {https://en.wikipedia.org/w/index.php?title=Haj%C3%B3s%27s_theorem&oldid=1041840894},
	abstract = {In group theory, Hajós's theorem states that if a finite abelian group is expressed as the Cartesian product of simplexes, that is, sets of the form 
  
    
      
