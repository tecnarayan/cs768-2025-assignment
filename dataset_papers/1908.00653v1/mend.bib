journal = {Theoretical Computer Science},
number = {C},
pages = {85--93},
title = {{NP is as easy as detecting unique solutions}},
volume = {47},
year = {1986}
}
@article{Belazzougui2016a,
abstract = {The Lempel-Ziv parsing of a string (LZ77 for short) is one of the most important and widely-used algorithmic tools in data compression and string processing. We show that the Lempel-Ziv parsing of a string of length {\$}n{\$} on an alphabet of size {\$}\backslashsigma{\$} can be computed in {\$}O(n\backslashlog\backslashlog\backslashsigma){\$} time ({\$}O(n){\$} time if we allow randomization) using {\$}O(n\backslashlog\backslashsigma){\$} bits of working space; that is, using space proportional to that of the input string in bits. The previous fastest algorithm using {\$}O(n\backslashlog\backslashsigma){\$} space takes {\$}O(n(\backslashlog\backslashsigma+\backslashlog\backslashlog n)){\$} time. We also consider the important rightmost variant of the problem, where the goal is to associate with each phrase of the parsing its most recent occurrence in the input string. We solve this problem in {\$}O(n(1 + (\backslashlog\backslashsigma/\backslashsqrt{\{}\backslashlog n{\}})){\$} time, using the same working space as above. The previous best solution for rightmost parsing uses {\$}O(n(1+\backslashlog\backslashsigma/\backslashlog\backslashlog n)){\$} time and {\$}O(n\backslashlog n){\$} space. As a bonus, in our solution for rightmost parsing we provide a faster construction method for efficient 2D orthogonal range reporting, which is of independent interest.},
