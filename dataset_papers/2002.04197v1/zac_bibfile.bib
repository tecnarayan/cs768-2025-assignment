@article{Gardner_OperationsSets_2013,
	title = {Operations between sets in geometry},
	volume = {15},
	doi = {10.4171/jems/422},
	abstract = {An investigation is launched into the fundamental characteristics of operations on and between sets, with a focus on compact convex sets and star sets (compact sets star-shaped with respect to the origin) in \$n\$-dimensional Euclidean space \${\textbackslash}mathbb \{R\}{\textasciicircum}n\$. It is proved that if \$n{\textbackslash}ge 2\$, with three trivial exceptions, an operation between origin-symmetric compact convex sets is continuous in the Hausdorff metric, \$GL(n)\$ covariant, and associative if and only if it is \$L\_p\$ addition for some \$1{\textbackslash}le p{\textbackslash}le {\textbackslash}infty \$. It is also demonstrated that if \$n{\textbackslash}ge 2\$, an operation \$*\$ between compact convex sets is continuous in the Hausdorff metric, \$GL(n)\$ covariant, and has the identity property (i.e., \$K*{\textbackslash}lbrace o{\textbackslash}rbrace =K={\textbackslash}lbrace o{\textbackslash}rbrace *K\$ for all compact convex sets \$K\$, where \$o\$ denotes the origin) if and only if it is Minkowski addition. Some analogous results for operations between star sets are obtained. Various characterizations are given of operations that are projection covariant, meaning that the operation can take place before or after projection onto subspaces, with the same effect. Several other new lines of investigation are followed. A relatively little-known but seminal operation called \$M\$-addition is generalized and systematically studied for the first time. Geometric-analytic formulas that characterize continuous and \$GL(n)\$-covariant operations between compact convex sets in terms of \$M\$-addition are established. It is shown that if \$n{\textbackslash}ge 2\$, an \$o\$-symmetrization of compact convex sets (i.e., a map from the compact convex sets to the origin-symmetric compact convex sets) is continuous in the Hausdorff metric, \$GL(n)\$ covariant, and translation invariant if and only if it is of the form \${\textbackslash}lambda DK\$ for some \${\textbackslash}lambda {\textbackslash}ge 0\$, where \$DK=K+(−K)\$ is the difference body of \$K\$. The term “polynomial volume” is introduced for the property of operations \$∗\$ between compact convex or star sets that the volume of \$rK*sL, r,s{\textbackslash}ge 0\$, is a polynomial in the variables \$r\$ and \$s\$. It is proved that if \$n{\textbackslash}ge 2\$, with three trivial exceptions, an operation between origin-symmetric compact convex sets is continuous in the Hausdorff metric, \$GL(n)\$ covariant, associative, and has polynomial volume if and only if it is Minkowski addition.},
	language = {eng},
	number = {6},
	journal = {Journal of the European Mathematical Society},
	author = {Gardner, Richard J. and Hug, Daniel and Weil, Wolfgang},
	year = {2013},
	keywords = {-addition, \$L\_p\$ addition, \$M\$-addition, addition, associativity equation, Brunn-Minkowski theory, central symmetral, compact convex set, difference body, Minkowski addition, polynomial volume, projection, radial addition, star set, symmetrization},
	pages = {2297--2352},
	file = {Gardner et al - Operations between sets in geometry (2013).pdf:/Users/zcranko/Documents/Library/Journal Article/Gardner et al - Operations between sets in geometry (2013).pdf:application/pdf}
}

