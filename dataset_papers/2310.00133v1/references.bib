	issn = {0027-8424},
	journal = {Proceedings of the National Academy of Sciences},
	number = {40},
	pages = {19848--19856},
	title = {High-resolution limited-angle phase tomography of dense layered objects using deep neural networks},
	volume = {116},
	year = {2019},
