@article{loomba_gut_2017,
	title = {Gut {Microbiome}-{Based} {Metagenomic} {Signature} for {Non}-invasive {Detection} of {Advanced} {Fibrosis} in {Human} {Nonalcoholic} {Fatty} {Liver} {Disease}},
	volume = {25},
	issn = {1932-7420},
	doi = {10.1016/j.cmet.2017.04.001},
	abstract = {The presence of advanced fibrosis in nonalcoholic fatty liver disease (NAFLD) is the most important predictor of liver mortality. There are limited data on the diagnostic accuracy of gut microbiota-derived signature for predicting the presence of advanced fibrosis. In this prospective study, we characterized~the gut microbiome compositions using whole-genome shotgun sequencing of DNA extracted from stool samples. This study included 86 uniquely well-characterized patients with biopsy-proven NAFLD, of which 72 had mild/moderate (stage 0-2 fibrosis) NAFLD, and 14 had advanced fibrosis (stage 3 or~4~fibrosis). We identified a set of 40 features (p~{\textless} 0.006), which included 37 bacterial species that were used to construct a Random Forest classifier model to distinguish mild/moderate NAFLD from advanced fibrosis. The model had a robust diagnostic~accuracy (AUC 0.936) for detecting advanced fibrosis. This study provides preliminary evidence for a fecal-microbiome-derived metagenomic signature to detect advanced fibrosis in NAFLD.},
	language = {eng},
	number = {5},
	journal = {Cell Metabolism},
	author = {Loomba, Rohit and Seguritan, Victor and Li, Weizhong and Long, Tao and Klitgord, Niels and Bhatt, Archana and Dulai, Parambir Singh and Caussy, Cyrielle and Bettencourt, Richele and Highlander, Sarah K. and Jones, Marcus B. and Sirlin, Claude B. and Schnabl, Bernd and Brinkac, Lauren and Schork, Nicholas and Chen, Chi-Hua and Brenner, David A. and Biggs, William and Yooseph, Shibu and Venter, J. Craig and Nelson, Karen E.},
	month = may,
	year = {2017},
	pmid = {28467925},
	pmcid = {PMC5502730},
	keywords = {Humans, Male, Adult, Aged, Female, Middle Aged, Prognosis, microbiome, Metagenomics, Feces, Gastrointestinal Microbiome, Prospective Studies, Bacteria, biomarker, cirrhosis, fatty liver, fibrosis, hepatic steatosis, hepatitis, Liver Cirrhosis, liver disease, NASH, Non-alcoholic Fatty Liver Disease, non-invasive},
