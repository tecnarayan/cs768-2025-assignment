	Issn = {1993-0623},
	Journal = {Journal of Control Theory and Applications},
	Number = {3},
	Pages = {336--352},
	Title = {A review of stochastic algorithms with continuous value function approximation and some new approximate policy iteration algorithms for multidimensional continuous applications},
	Url = {https://doi.org/10.1007/s11768-011-0313-y},
