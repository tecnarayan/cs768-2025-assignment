	keywords = {Humans, Genomics, High-Throughput Nucleotide Sequencing, Computational Biology, Reproducibility of Results, Genetic Testing, Reference Standards, Quality Control, DNA Barcoding, Taxonomic, Plasmids, Workflow},
	pages = {336--349},
