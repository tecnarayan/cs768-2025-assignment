@article{mcbride_retooling_2016,
	title = {Retooling {Poverty} {Targeting} {Using} {Out}-of-{Sample} {Validation} and {Machine} {Learning}},
	url = {https://academic.oup.com/wber/article/doi/10.1093/wber/lhw056/2447896/Retooling-Poverty-Targeting-Using-Out-of-Sample},
