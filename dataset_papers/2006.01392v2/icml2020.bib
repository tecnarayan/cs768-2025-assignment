	month = apr,
	year = {2003},
	pages = {279--300},
	file = {Full Text PDF:/home/thom/Zotero/storage/DUTXZ9RA/Egozcue et al. - 2003 - Isometric Logratio Transformations for Composition.pdf:application/pdf;Snapshot:/home/thom/Zotero/storage/SVTXBXPQ/A1023818214614.html:text/html}
}

