@article{KhachiyanMVE,
 ISSN = {0364765X, 15265471},
 URL = {http://www.jstor.org/stable/3690235},
 abstract = {Let A be a set of m points in Rn. We show that the problem of (1 + ε)n-rounding of A, i.e., the problem of computing an ellipsoid $E\subseteq {\Bbb R}^{n}$ such that $[(1+\epsilon )n]^{-1}$ E ⊆ conv. hull(A) ⊆ E, can be solved in $O(mn^{2}(\epsilon ^{-1}+{\rm ln}\,n+{\rm ln}\,{\rm ln}\,m))$ arithmetic operations and comparisons. This result implies that the problem of approximating the minimum volume ellipsoid circumscribed about A can be solved in $O(m^{3.5}\,{\rm ln}(m\epsilon ^{-1}))$ operations to a relative accuracy of ε in the volume. The latter bound also applies to the (1 + ε)n-rounding problem. Our bounds hold for the real number model of computation.},
 author = {Leonid G. Khachiyan},
 journal = {Mathematics of Operations Research},
 number = {2},
 pages = {307--320},
 publisher = {INFORMS},
 title = {Rounding of Polytopes in the Real Number Model of Computation},
 volume = {21},
 year = {1996}
}


