	volume = {25},
	issn = {1061-8600},
	shorttitle = {Displaying {Variation} in {Large} {Datasets}},
	doi = {10.1080/10618600.2015.1131161},
	abstract = {Displaying the component-wise between-group differences high-dimensional datasets is problematic because widely used plots such as Bland{\textendash}Altman and Volcano plots do not show what they are colloquially believed to show. Thus, it is difficult for the experimentalist to grasp why the between-group difference of one component is {\textquotedblleft}significant{\textquotedblright} while that of another component is not. Here, we propose a type of {\textquotedblleft}Effect Plot{\textquotedblright} that displays between-group differences in relation to respective underlying variability for every component of a high-dimensional dataset. We use synthetic data to show that such a plot captures the essence of what determines {\textquotedblleft}significance{\textquotedblright} for between-group differences in each component, and provide guidance in the interpretation of the plot. Supplementary online materials contain the code and data for this article and include simple R functions to produce an effect plot from suitable datasets.},
	number = {3},
	urldate = {2017-03-06},
	journal = {Journal of Computational and Graphical Statistics},
	author = {Gloor, Gregory B. and Macklaim, Jean M. and Fernandes, Andrew D.},
	month = jul,
	year = {2016},
	keywords = {Genomics, transcriptomics, ANOVA, Bland{\textendash}Altman plot, Multivariate datasets, Volcano plot},
	pages = {971--979},
	file = {Full Text PDF:/home/thom/Zotero/storage/Z6N358IZ/Gloor et al. - 2016 - Displaying Variation in Large Datasets Plotting a.pdf:application/pdf;Snapshot:/home/thom/Zotero/storage/3465KDHR/10618600.2015.html:text/html}
}

@article{buccianti_is_2013,
	series = {Benchmark problems, datasets and methodologies for the computational geosciences},
	title = {Is compositional data analysis a way to see beyond the illusion?},
	volume = {50},
	issn = {0098-3004},
	doi = {10.1016/j.cageo.2012.06.012},
	abstract = {Notwithstanding the numerous contributions that have been published on theoretical and practical aspects of the management of compositional (constrained) data during the last thirty years, in geochemistry most of the scientific papers in international journals continue to ignore their peculiar features. In order to understand the reasons of the undervaluation of the effects of an incorrect choice of the sample space and, consequently, an incorrect application of the distance concept, case studies of comparison between methodologies will be presented and discussed. The aim is to evaluate the differences in interpretation of geochemical processes affecting rocks, water and gaseous samples, when the two different approaches, classical and compositional, are adopted. If we compare the results of case studies following the two paths it is possible to evaluate which type of error (and consequences) will affect our evaluations in geochemistry.
The presence of expected differences between the two approaches indicates that compositional data analysis can be a way to see beyond the illusion due to the constrained space. However, the possibility that the difference is tenuous in some situations, not revealed a priori, may be at the origin of the unconscious choice of the classical approach. Is this condition which some researchers call {\textquotedblleft}common sense{\textquotedblright} frequently encountered in geochemistry? The paper is aimed to try to answer the proposed question, and to understand the difficulty of diffusion of compositional data analysis even if now simple tools of investigation, for different degrees of knowledge, are available.},
	urldate = {2017-03-06},
	journal = {Computers \& Geosciences},
	author = {Buccianti, Antonella},
	month = jan,
	year = {2013},
	keywords = {Compositional data analysis, Geochemistry, Isometric log-ratio transformation, River chemistry, Volcanic gases, Worldwide lithology},
	pages = {165--173},
	file = {ScienceDirect Full Text PDF:/home/thom/Zotero/storage/HAS8CDSD/Buccianti - 2013 - Is compositional data analysis a way to see beyond.pdf:application/pdf;ScienceDirect Snapshot:/home/thom/Zotero/storage/FVJDK9QV/S0098300412002075.html:text/html}
}

